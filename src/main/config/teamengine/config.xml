<?xml version="1.0" encoding="UTF-8"?>
<!-- Unpack the *-ctl-scripts.zip archive in the TE_BASE/scripts directory -->
<!-- Add this entry to TE_BASE/config.xml (//organization/standard) -->
<organization>
  <name>OGC</name>
  <standard>
    <name>Observations and Measurements (OM)</name>
    <version>
      <name>${spec-version}</name>
      <suite>
        <namespace-uri>http://www.opengis.net/cite/om20</namespace-uri>
        <prefix>tns</prefix>
        <local-name>ets-om20-${version}</local-name>
        <title>Observations and Measurements (OM)</title>
        <description>Checks om instances for conformance to OGC 10-025r1.</description>
        <BasicConformanceClasses>
		  <conformanceClass>BasicConformanceClass</conformanceClass>
		</BasicConformanceClasses>
      </suite>
      <revision>
        <name>${version}</name>
        <status>Beta</status>
        <sources>
          <source>om20/${spec-version}/ctl</source>
        </sources>
        <webdir>om20/${spec-version}/site</webdir>
      </revision>
    </version>
  </standard>
</organization>
